{
  "name": "@groveengine/cli",
  "version": "0.1.0",
  "description": "CLI for the Grove ecosystem - interact with Lattice, Amber, Bloom and more",
  "type": "module",
  "bin": {
    "grove": "./dist/cli/index.js"
  },
  "scripts": {
    "dev": "wrangler dev",
    "dev:cli": "npx tsx cli/index.ts",
    "build:cli": "tsc -p tsconfig.cli.json",
    "deploy": "wrangler deploy",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "typecheck:cli": "tsc -p tsconfig.cli.json --noEmit",
    "lint": "eslint src/ cli/ core/",
    "format": "prettier --write src/ cli/ core/"
  },
  "dependencies": {
    "@cloudflare/workers-oauth-provider": "^0.0.1",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "agents": "^0.3.3",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "keytar": "^7.9.0",
    "ora": "^8.1.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250101.0",
    "@types/node": "^22.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.4.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0",
    "wrangler": "^4.54.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "mcp",
    "cloudflare",
    "workers",
    "durable-objects",
    "grove",
    "ai-agents",
    "cli"
  ],
  "author": "Autumn Brown",
  "license": "MIT"
}
